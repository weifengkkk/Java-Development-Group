<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd
           http://www.springframework.org/schema/aop
           http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
    <context:component-scan base-package="com.bee"/>
    <aop:aspectj-autoproxy></aop:aspectj-autoproxy>

    <bean class="com.bee.dao.MysqlDao" name="Mysql"></bean>
    <bean class="com.bee.dao.OracleDao" name="Oracle"></bean>
    <bean class="com.bee.service.TuserServiceImp" name="tuserService"></bean>
<!--        <property name="dao" ref="Oracle"></property>-->

</beans>